.tournament {      
    background-color: #F0F0F0;  
    border: dashed 1px solid;  
    overflow: auto;  
  }  
.bracket1 {
	width: 50%;
	float: left;
}

.bracket3 {
	float: right;
	width: 50%;
	text-align: right;

}
 
.round {  
    background-color: #DFDFDF;  
    min-width: 100px;      
    vertical-align: top;  
    float: left;  
  }  
    
.match {  
    background-color: #D0D0D0;  
    border-top: 1px solid;  
    border-right: 1px solid;  
    border-bottom: 1px solid;    
  }  
.p1 {      
    height: 20px;  
  }  
.p2 {  
    height: 20px;  
  }      
.match_spacer {  
    background-color: #DFDFDF;  
    height: 38px;  
  }  
  .tournament .round .spacer {  
    height: 80px;  
  }  
  .tournament .round .half-spacer {  
    height: 40px;  
  }  
.small-spacer {  
    height: 10px;  
    background-color: #F1F1F1;  
  }  
  .tournament .round .winner {  
    border-bottom: 1px solid;  
  }  
    
  .left-line {  
    border-left: 1px solid;  
  }  
    
  .tournament .cell {  
    min-width: 100px;  
    height: 20px;  
    float: left;  
    background-color: #DFDFDF;      
  }     
  .tournament .l2 {  
    background-color: #D0D0D0;  
  }       
  .tournament .lmax {  
    width: 0px;  
    clear: both;  
  } 

#MidWest h1 {
	text-decoration: underline;
	font-size: 30px;
}
#MidWest h2 {
	font-size: 20px;
	text-align: center;
	margin-bottom: 0px;
}
#East {
	float: right;
}

#East h1 {
        text-decoration: underline;
        font-size: 30px;
}
#East h2 {
        font-size: 20px;
        text-align: center;
        margin-bottom: 0px;
}

.bracket3_round {
	background-color: #DFDFDF;
	min-width: 100px;
	vertical-align: top;
	float: right;
}

.bracket3_match {
	float: right;
	width: 100px;
	background-color: #D0D0D0;
	border-top: 1px solid;
	border-left: 1px solid;
	border-bottom: 1px solid;

}

#bracket3-spacer {
	clear: both;
}

#round1_spacer {
	height: 0px;
}

#round2_spacer {
        height: 38px;
